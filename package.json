{
  "name": "burndown-front",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "concurrently \" yarn dev:server\" \" yarn dev:front \"",
    "dev:server": "ts-node-dev --respawn --transpile-only src/server/index.ts",
    "dev:front": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.20.0",
    "cors": "^2.8.5",
    "date-fns": "^2.16.1",
    "express": "^4.17.1",
    "puppeteer": "^5.3.0",
    "react": "^16.13.1",
    "react-day-picker": "^7.4.8",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3",
    "recharts": "^1.8.5",
    "concurrently": "^5.3.0",
    "ts-node-dev": "^1.0.0-pre.63"
  },
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/puppeteer": "^3.0.2",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/recharts": "^1.8.15",
    "typescript": "~3.7.2"
  }
}
